<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_quant_tree_editable_inherit_gross_weight" model="ir.ui.view">
        <field name="name">stock.quant.view.form.inherit</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='available_quantity']" position="after">
                <field name="gross_weight"/>
                <field name="cone_quantity"/>
            </xpath>
        </field>
    </record>
</odoo>